<struts>
	<!-- 员工配置 -->
	<package name="cou" namespace="/" extends="common">
		<action name="courseTypeAction_*" class="com.hsq.web.action.CourseTypeAction" method="{1}">
			<result name="findAll">/WEB-INF/pages/coursetype/listCourse.jsp</result>
